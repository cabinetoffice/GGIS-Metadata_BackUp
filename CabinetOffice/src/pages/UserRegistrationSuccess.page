<apex:page showHeader="false" sidebar="false" cache="false">
	<head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <link rel="stylesheet" href="{!URLFOR($Resource.DT_Custom_Register_Bundle, 'css/registration.css')}"/>
    </head>
    <body id="co-body-wrapper">
        <div id="co-row-wrapper">
            <div id="co-content">
                <div class="container ">
                    <div class="row">
                        <div class="col-xs-12" style="text-align:center;margin-bottom:10px;">
                            <img width="210" height="80" src="{!URLFOR($Resource.DT_Custom_Login_Bundle, 'img/co-logo.png')}" alt=""/>
                        </div>
                        <div id="co-register-form-wrapper" class="col-xs-10 col-xs-offset-1 col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 col-lg-8 col-lg-offset-2 box-shadow--6dp" style="height:100%;">
                        	<center>
                        		<div id="co-register-form" class="row">
                        			<div class="col-sm-12" style="font-size:20px;">
                                        <apex:outputText value="{!$Label.DataSolutionRegistrationSuccessMessage}" escape="false"/>
						    		</div>
						    	</div>
                            </center>
                        </div>
                    </div>
                    <div id="co-footer-wrapper" class="row">
                        <apex:outputText value="{!$Label.Data_Solution_Login_Footer_Text}" escape="false"/>
                    </div>
                </div>
            </div>
        </div>
    </body>
</apex:page>